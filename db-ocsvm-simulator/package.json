{
  "name": "db-ocsvm-gui",
  "description": "A GUI for the DB-OCSVM anomaly detection tool",
  "author": {
    "name": "DB-OCSVM Group Mates"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run build:electron && cross-env NODE_ENV=development electron .",
    "build:react": "tsc -b && vite build",
    "build:electron": "tsc --project src/main/tsconfig.json && node ./electron.ml.config.js",
    "transpile:electron": "tsc --project src/main/tsconfig.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "dist:win": "npm run build:react && npm run build:electron && electron-builder --win --x64"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.462.0",
    "onnxruntime-node": "^1.20.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.1",
    "recharts": "^2.15.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.24",
    "electron": "^33.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
